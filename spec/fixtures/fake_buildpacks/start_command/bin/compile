#!/usr/bin/env bash

echo "-----> Some compilation output"
if [ -d $2 ]; then
  echo "-----> Copying files from cache"
  mv $2/* $1

  echo "-----> Saving files to cache"
  touch $2/new_cached_file
fi

if [ "$FOO" = "BAR" ] && [ "$BLAH" = "WHATEVER" ]; then
  echo "-----> Running foo based script"
fi
exit 0
